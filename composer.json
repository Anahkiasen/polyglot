{
  "name": "anahkiasen/polyglot",
  "description": "Laravel localization helpers and models",
  "license": "MIT",
  "keywords": [
    "laravel",
    "localization",
    "translation",
    "model"
  ],
  "authors": [
    {
      "name": "Maxime Fabre",
      "email": "ehtnam6@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "laravel/framework": "5.3.*"
  },
  "require-dev": {
    "laravel/laravel": "5.3.*",
    "phpunit/phpunit": "^4.8",
    "fabpot/php-cs-fixer": "^1.10",
    "mockery/mockery": "^0.9.4",
    "patchwork/utf8": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Polyglot\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Polyglot\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit --process-isolation"
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "2.0-dev"
    }
  }
}
