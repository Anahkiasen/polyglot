{
  "name": "anahkiasen/polyglot",
  "description": "Laravel localization helpers and models",
  "license": "MIT",
  "keywords": [
    "laravel",
    "localization",
    "translation",
    "model"
  ],
  "authors": [
    {
      "name": "Maxime Fabre",
      "email": "ehtnam6@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/container": "~4.2",
    "illuminate/support": "~4.2"
  },
  "require-dev": {
    "fabpot/php-cs-fixer": "2.0.*@dev",
    "illuminate/database": "~4.2",
    "illuminate/routing": "~4.2",
    "illuminate/translation": "~4.2",
    "mockery/mockery": "^0.9.4",
    "patchwork/utf8": "^1.2",
    "phpunit/phpunit": "^4.7"
  },
  "autoload": {
    "psr-4": {
      "Polyglot\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Polyglot\\": "tests"
    }
  }
}
